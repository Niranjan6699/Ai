# bfs.py
from collections import deque
def bfs(graph, start):
    visited = set([start])
    order = []
    q = deque([start])
    while q:
        u = q.popleft()
        order.append(u)
        for v in graph.get(u, []):
            if v not in visited:
                visited.add(v); q.append(v)
    return order

if __name__=="__main__":
    g = {'A':['B','C'],'B':['D'],'C':['E'],'D':[],'E':[]}
    print(bfs(g,'A'))
